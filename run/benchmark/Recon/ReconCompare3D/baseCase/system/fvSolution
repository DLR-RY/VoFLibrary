FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    pcorr
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-10;
        relTol          0;
    }

    p_rgh
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-07;
        relTol          0.05;
    }

    p_rghFinal
    {
        $p_rgh;
        tolerance       1e-07;
        relTol          0;
    }

    U
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-06;
        relTol          0;
    }

    alpha1
    {
        solver          PBiCG;
        preconditioner  DILU;
        tolerance       1e-06;
        relTol          0;

	//reconstructionScheme gradAlpha;
	reconstructionScheme RECONSCHEME;
	//gradAlphaCoeffs
    interpolateNormal false;
    iterations 10;

	vof2IsoTol 1e-8;
	surfCellTol 1e-8;
	writeVTK false;

    }
}

isoAdvector
{
    isoFaceTol	1e-8;
    surfCellTol	1e-8;
    clipAlphaTol 0;
    nAlphaBounds 3;
    snapAlpha	false;
}

//reconstructionScheme gradAlpha;
reconstructionScheme RECONSCHEME;
//gradAlphaCoeffs

	vof2IsoTol 1e-8;
	surfCellTol 1e-8;
	writeVTK false;
    interpolateNormal false;
    iterations 10;


PIMPLE
{
    momentumPredictor no;
    nCorrectors     -1;
    nNonOrthogonalCorrectors -1;
    nAlphaCorr      1;
    nAlphaSubCycles 1;
    cAlpha          1;
    pRefCell        0;
    pRefValue       0;
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
}

